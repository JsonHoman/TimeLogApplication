@model Job

@{
    ViewData["Title"] = "Create Job";
}


<form asp-action="CreateJob" method="post">
    <div>
        <label>Job Title: </label>
        <input class="btn btn-default" type="text" asp-for="Title" />
    </div>
    <br />
    <div style="padding-left:100px">
        <input type="submit" value="Create Job" class="btn btn-default" />
    </div>
</form>

